{
  "name": "angular-blog-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve web",
    "build": "ng build web",
    "build:all": "ng build shared && ng build web && ng build admin && ng build code-samples-mfe",
    "watch": "ng build --watch --configuration development",
    "test": "ng test web",
    "test:all": "ng test shared && ng test web && ng test admin && ng test code-samples-mfe",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write",
    "serve:ssr:web": "node dist/web/server/server.mjs",
    "build:stats": "ng build web --stats-json",
    "analyze": "webpack-bundle-analyzer dist/web/stats.json",
    "start:local": "ng serve web --configuration=local",
    "start:local:docker": "open -a Docker",
    "start:local:backend": "npx supabase start",
    "schema:pull": "find supabase/migrations -name '*_remote_schema.sql' -delete && supabase db pull --db-url $PG_EXPORT_URL",
    "db:createSeed": "scripts/create-seed.sh",
    "db:seed": "npx @snaplet/seed init",
    "users:passwords": "scripts/set-passwords.sh",
    "routes:generate": "node scripts/generate-routes.js",
    "routes:generate:ts": "tsx scripts/generate-routes.ts",
    "e2e": "playwright test",
    "e2e:web": "playwright test --project=web",
    "e2e:web:local": "playwright test --project=web --project=local",
    "e2e:admin": "playwright test --project=admin",
    "e2e:admin:local": "playwright test --project=admin",
    "lint": "npm run lint:projects",
    "lint:fix": "npm run lint:projects -- --fix",
    "lint:all": "ng lint admin && ng lint web && ng lint code-samples-mfe",
    "lint:all:fix": "ng lint admin --fix && ng lint web --fix && ng lint code-samples-mfe --fix",
    "lint:boundaries": "eslint --ext .ts,.html projects",
    "lint:projects": "ng lint admin && ng lint web && ng lint code-samples-mfe",
    "lint:admin": "ng lint admin",
    "lint:admin:fix": "ng lint admin --fix",
    "lint:web": "ng lint web",
    "lint:web:fix": "ng lint web --fix",
    "lint:mfe": "ng lint code-samples-mfe",
    "lint:mfe:fix": "ng lint code-samples-mfe --fix",
    "commit:web": "echo 'Use: git commit -m \"feat(web): your message\"'",
    "commit:admin": "echo 'Use: git commit -m \"feat(admin): your message\"'", 
    "commit:mfe": "echo 'Use: git commit -m \"feat(mfe): your message\"'",
    "commit:shared": "echo 'Use: git commit -m \"feat(shared): your message\"'",
    "commit:config": "echo 'Use: git commit -m \"chore(config): your message\"'",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.7",
    "@angular/common": "^20.1.7",
    "@angular/compiler": "^20.1.7",
    "@angular/core": "^20.1.7",
    "@angular/forms": "^20.1.7",
    "@angular/platform-browser": "^20.1.7",
    "@angular/platform-browser-dynamic": "^20.1.7",
    "@angular/platform-server": "^20.1.7",
    "@angular/router": "^20.1.7",
    "@angular/ssr": "^20.1.6",
    "@ngrx/operators": "^20.0.0",
    "@ngrx/signals": "^20.0.0",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/line-clamp": "^0.4.4",
    "express": "^5.1.0",
    "i": "^0.3.7",
    "ngx-highlightjs": "^12.0.0",
    "ngx-quill": "^28.0.1",
    "node-fetch": "^3.3.2",
    "npm": "^10.8.2",
    "quill": "^2.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.7",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.53.0",
    "@snaplet/copycat": "^6.0.0",
    "@snaplet/seed": "^0.98.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "@types/pg": "^8.15.2",
    "angular-eslint": "20.1.1",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.10",
    "esbuild-visualizer": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.32.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.1.0",
    "pg": "^8.16.0",
    "playwright-ng-schematics": "^2.0.3",
    "postcss": "^8.4.38",
    "prettier": "3.4.2",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.34.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  }
}
